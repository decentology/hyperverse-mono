{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "React Library",
	"extends": "./base.json",
	"include": ["source/**/*"],
	"exclude": ["**/node_modules/*"],
	"compilerOptions": {
		"lib": ["ES2017", "DOM"],
		"module": "ESNext",
		"target": "ES6",
		"rootDir": "../",
		"preserveSymlinks": true,
		"outDir": "distribution",
		"importHelpers": false,
		"moduleResolution": "Node",
		"jsx": "react-jsx"
	}
}
